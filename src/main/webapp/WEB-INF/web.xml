<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">
  <servlet>
    <servlet-name>donnees</servlet-name>
    <servlet-class>com.sullivan.website.admin.servlets.donneesservlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>donnees</servlet-name>
    <url-pattern>/admin/donnees</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>getUsersServlet</servlet-name>
    <servlet-class>com.sullivan.website.users.servlets.GetUsersServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getUsersServlet</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>getusersconnect</servlet-name>
    <servlet-class>com.sullivan.website.users.servlets.getusersconnect</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getusersconnect</servlet-name>
    <url-pattern>/admin/users</url-pattern>
  </servlet-mapping>


  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
